INSERT INTO `node_type` VALUES ('navagation_menu_item','Navagation Menu Item','node','<p>Used for navagational structure only. Content is blank and will have no User or Date/Time stamp.</p>','','1','Title','0','','0','1','1','0',''),('how_to_page','How-to page','node','<p>A <em>How-to page</em> is used where a user needs information on how to perform a specific function or task. It is a page of content, formatted specifically for that page type, ie, Desired Result; Applies To; Required Steps. A <em>how-to page</em> automatically displays links to adjacent pages, providing a simple navigation system for organizing and reviewing structured content.</p>','','1','Desired Result','0','','0','1','1','0',''),('troubleshooting_page','Troubleshooting page','node','<p>A <em>Troubleshooting Page</em> is used where a user has a problem and needs a workaround or other information. It is a page of content, formatted specifically for that page type, ie, Problem Description; Applies To; Symptoms; Cause; and Solution. A <em>Troubleshooting Page</em> automatically displays links to adjacent pages, providing a simple navigation system for organizing and reviewing structured content.</p>','','1','Problem Description','0','','0','1','1','0','book'),('q_and_a_page','Q and A page','node','<p>A&nbsp;<em>Q &amp; A page</em> is used in content other than FAQ\'s. It is a page of content, formatted specifically for that page type, ie, Question; Answer. A&nbsp;<em>Q&amp;A page</em> automatically displays links to adjacent pages, providing a simple navigation system for organizing and reviewing structured content.</p>','','1','Question','0','','0','1','1','0',''),('general_information_page','General information page','node','<p>A <em>General information page</em> is used where a user needs general information about a feature, the solution, etc. It is a page of content, formatted specifically for that page type, ie, Subject; Information. A <em>General information page</em> automatically displays links to adjacent pages, providing a simple navigation system for organizing and reviewing structured content.</p>','','1','Subject','0','','0','1','1','0','');
